#  Units:
#  1f = current font size
#  1s = current width of the spache character
#  1v = current inter-line spacing
#  c  = cm     i  = inches   p  = points (72 pts = 1i)
#  m  = ems (12m = 1i)
#
@Include { sr_doc_plaintext }
@Doc @Text @Begin
  
@B @S { @sr_label } 

@PP

@PP @dateline

@PP

15c @Wide lines @Break { @sr_text }
        
@PP
@LLP @signature_a
@LLP @signature_b
@LLP @header_a
@LLP @header_b
@LLP @header_c
@LLP @header_d
@LLP @header_e
@LLP @header_f
@LLP @header_g


          
@End @Text

