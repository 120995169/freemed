<SimpleUIBuilder module="DosingStation">
	<Elements>
		<Element field="dsname" title="Dosing Station Name" type="TEXT" options="50" display="1" />
		<Element field="dslocation" title="Location" type="TEXT" options="75" />
		<Element field="dsfacility" title="Facility" type="TEXT" options="75" />
		<Element field="dsusername" title="Username" type="TEXT" options="50" />
		<Element field="dspassword" title="Password" type="TEXT" options="50" />
		<Element field="dsurl" title="Station WSDL URL" type="TEXT" options="75" display="1" />
		<Element field="dsenabled" title="Enabled" type="SELECT" options="True|1,False|0" />
		<Element field="dsopen" title="Status" type="SELECT" options="Close|closed,Open|open" />
	</Elements>
</SimpleUIBuilder>
